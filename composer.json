{
  "name": "phpDockerizedApp",
  "description": "Sample of a dockerized php application.",
  "require": {
    "php": "^7.1",
    "carlosbuenosvinos/ddd": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Status\\": "Status/src/"

    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload-dev": {
    "psr-4": {
      "StatusTest\\": "Status/tests/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "preferred-install": "dist"
  }
}
